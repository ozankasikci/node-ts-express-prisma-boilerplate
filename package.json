{
  "name": "node-ts-express-prisma-boilerplate",
  "version": "1.0.0",
  "description": "Production-ready Node.js boilerplate with TypeScript, Express, Prisma, Redis, and BullMQ",
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsup",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "worker": "node dist/workers/task-worker.js",
    "worker:dev": "nodemon --exec tsx src/workers/task-worker.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "template",
    "boilerplate",
    "redis",
    "bullmq",
    "jwt",
    "authentication"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.1",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.63.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^10.1.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "pg-mem": "^3.0.2",
    "prettier": "^3.6.0",
    "prisma": "^6.19.0",
    "supertest": "^7.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.0"
  }
}
